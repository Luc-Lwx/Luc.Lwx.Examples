{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Lwx": 
  {
      "ApiManagerPath": "/apimanager-prefix"
  },
  "LwxCors": {
    "AllowedOrigins": "https://example.com,https://another.example.com",
    "AllowedMethods": "GET, POST, PUT, DELETE",
    "AllowedHeaders": "Content-Type, Authorization"
  },
  "LwxActivityLog": {
    "FixIpAddr": true,
    "IgnoreEndpointsWithoutAttribute": true,
    "ErrorHandler": true
  }
}
